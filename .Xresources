! Xft settings ---------------------------------------------------------
Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
!Xft.autohint: 0

! special
*.foreground:   #dbdcd9
*.background:   #0d0e0f
*.cursorColor:  #dbdcd9

! black
*.color0:       #1c1d1e
*.color8:       #373b41

! red
*.color1:       #c13333
*.color9:       #cc6666

! green
*.color2:       #4f9440
*.color10:      #b5bd68

! yellow
*.color3:       #deb940
*.color11:      #f0c674

! blue
*.color4:       #17578b
*.color12:      #81a2be

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #a9abae
*.color15:      #e2e5e9

! URxvt -----------------------------------------------------------------------
URxvt*termName:          rxvt-256color
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false

URxvt*transparent:       false

URxvt*depth:             32
URxvt.background:        [90]#111112

!URxvt.font:              xft:Inconsolata LGC Markup:size=11:antialias=true:autohint=true
!URxvt.boldFont:          xft:Inconsolata LGC Markup:size=11:antialias=true:autohint=true
URxvt.font:              xft:Fira Code:Retina:size=11:antialias=true:autohint=true
URxvt.boldFont:          xft:Fira Code:Retina:size=11:antialias=true:autohint=true
URxvt.letterSpace: 1

*antialias:              true
*autohint:               true

!URxvt.perl-ext-common: default,matcher,selection-to-clipboard
!URxvt.url-launcher: firefox
!URxvt.matcher.button: 1
URxvt.perl-ext-common:  matcher
URxvt.urlLauncher:      firefox
URxvt.tabbed.tabbar-fg: 14
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    13
URxvt.tabbed.tab-bg:    0


! XTerminals ------------------------------------------------------------
UXterm*termName:            xterm-256color
!UXTerm*faceName:            xft:Hack:size=11:antialias=true:autohint=true
UXTerm*faceName:            xft:Roboto Mono:size=11:antialias=true:autohint=true
UXTerm*utf8:                1
!UXTerm*font:               -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!UXTerm*boldFont:           -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!UXTerm*background:         #101012
!UXTerm*foreground:         #E5E5E5
!UXTerm*cursorColor:        #E5E5E5
UXTerm*scrollBar:          false
UXTerm*rightScrollBar:     false

XTerm*termName:           xterm-256color
!XTerm*background:         #101012
!XTerm*foreground:         #E5E5E5
!XTerm*cursorColor:        #E5E5E5
!XTerm*font:               -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!XTerm*boldFont:           -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
XTerm*faceName:          xft:Roboto Mono:size=11:antialias=true:autohint=true
XTerm*utf8:               1
XTerm*scrollBar:          false
XTerm*rightScrollBar:     false

! X-11 Apps------------------------------------------------------------------
xpdf*Background:    #CFC6BE
xpdf*Foreground:    #050505
xpdf*FontList:      -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
xpdf*initialZoom:   page
xpdf*urlCommand:    firefox %s
xpdf*textEncoding:  utf-8

