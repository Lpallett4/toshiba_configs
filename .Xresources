! Xft settings ---------------------------------------------------------
Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
!Xft.autohint: 0

*background:  #303030
*colorUL:  #ffffef
*cursorColor:  #b2e9ef
*foreground:  #f5f5f5
*underlineColor:  #ffffef
*xtermbackground:  #126811
*color0:  #22300f
*color1:  #cf7d68
*color2:  #92ff05
*color3:  #cfff05
*color4:  #737dbb
*color5:  #cf7db9
*color6:  #19ffbb
*color7:  #c1c3be
*color8:  #688466
*color9:  #e0c676
*color10:  #a2ff50
*color11:  #e0ff4e
*color12:  #82c6cc
*color13:  #bbc6cc
*color14:  #62ffa4
*color15:  #ffffff

! URxvt -----------------------------------------------------------------------
URxvt*termName:          rxvt-256color
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false

URxvt*transparent:       false

URxvt*depth:             32
URxvt.background:        [90]#0F0F0F

URxvt.font:              xft:Inconsolata LGC Markup:size=11:antialias=true:autohint=true
URxvt.boldFont:          xft:Inconsolata LGC Markup:size=11:antialias=true:autohint=true
!URxvt.font:              xft:Liberation Mono:size=11:antialias=true:autohint=true
!URxvt.boldFont:          xft:Liberation Mono:size=11:antialias=true:autohint=true
URxvt.letterSpace: 1

*antialias:              true
*autohint:               true

!URxvt.perl-ext-common: default,matcher,selection-to-clipboard
!URxvt.url-launcher: firefox
!URxvt.matcher.button: 1
URxvt.perl-ext-common:  matcher
URxvt.urlLauncher:      firefox
URxvt.tabbed.tabbar-fg: 14
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    13
URxvt.tabbed.tab-bg:    0


! XTerminals ------------------------------------------------------------
UXterm*termName:            xterm-256color
UXTerm*faceName:            xft:Liberation Mono:size=11:antialias=true:autohint=true
UXTerm*utf8:                1
!UXTerm*font:               -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!UXTerm*boldFont:           -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
UXTerm*background:         #171717
UXTerm*foreground:         #E5E5E5
UXTerm*cursorColor:        #E5E5E5
UXTerm*scrollBar:          false
UXTerm*rightScrollBar:     false

XTerm*termName:           xterm-256color
XTerm*background:         #171717
XTerm*foreground:         #E5E5E5
XTerm*cursorColor:        #E5E5E5
!XTerm*font:               -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!XTerm*boldFont:           -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
XTerm*faceName:          xft:Liberation Mono:size=11:antialias=true:autohint=true
XTerm*utf8:               1
XTerm*scrollBar:          false
XTerm*rightScrollBar:     false

! X-11 Apps------------------------------------------------------------------
xpdf*Background:    #CFC6BE
xpdf*Foreground:    #050505
xpdf*FontList:      -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
xpdf*initialZoom:   page
xpdf*urlCommand:    firefox %s
xpdf*textEncoding:  utf-8

!source: https://wiki.archlinux.org/index.php/X_resources
