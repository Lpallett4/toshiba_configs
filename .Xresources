! xosview
xosview*geometry: 300x210+2+2
xosview*font: -xos4-terminus-medium-r-normal-*-14-140-*-*-*-*-iso8859-1
xosview*background: #E5E5EE
xosview*foreground: #202020
xosview*meterLabelColor: #202020
xosview*usedlabels: true
xosview*usedLabelColor: #202020
xosview*enableStipple: false
!xosview*graphNumCols:
xosview*load: true
xosview*loadIdleColor: #BBBBBB
xosview*loadProcColor: #2A296D
xosview*loadWarnColor: #D7AE2E
xosview*loadCritColor: #DA1613
!xosview*loadIdleColor:
xosview*loadPriority: 30
xosview*cpu: true
xosview*cpuPriority: 30
xosview*cpuUsedFormat: percent
xosview*cpuFormat: single
xosview*cpuFreeColor: #BBBBBB
xosview*mem: true
xosview*memPriority: 30
xosview*memFreeColor: #BBBBBB
!
xosview*swap: false
xosview*page: false
xosview*gfx: false
xosview*disk: false
!
xosview*acpitemp: true
xosview*battery: true
xosview*batteryLeftColor: #2A296D
xosview*batteryUsedColor: #4e4e4e
!xosview*batteryChargeColor:
xosview*batteryFullColor: #2A296D
xosview*batteryLowColor: #D7AE2E
xosview*batteryCritColor: #DA1613
xosview*batteryUsedFormat: percent
xosview*batteryPriority: 30
xosview*wireless: true
xosview*PoorQualityColor: #DA1613
xosview*FairQualityColor: #D7AE2E
xosview*GoodQualityColor: #2A296D
xosview*wirelessPriority: 30
xosview*net: true
xosview*netIface: wlp1s0
xosview*netPriority: 30

xosview*cpuGraph: false
xosview*loadGraph: false









